[
    {
      "define_var": {
        "call": "var.write",
        "args": {
          "input_path": "examples/array.csv",
          "read_as": "csv"
        },
        "result": "array_var"
      }
    },
    {
      "define": {
        "assign": [
          {
            "array": "${array_var.array}"
          },
          {
            "result": ""
          },
          {
            "i": 0
          }
        ]
      }
    },
    {
      "check_condition": {
        "switch": [
          {
            "condition": "${len(array) > i}",
            "next": "iterate"
          }
        ],
        "next": "exit_loop"
      }
    },
    {
      "iterate": {
        "assign": [
          {
            "result": "${result + array[i]}"
          },
          {
            "i": "${i+1}"
          }
        ],
        "next": "check_condition"
      }
    },
    {
      "exit_loop": {
        "return": {
          "concat_result": "${result}"
        }
      }
    }
]